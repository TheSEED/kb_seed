{
    "iris": {
	"group-names":  [
                {"group-name": "cs", "description": "Central Store Scripts"},
                {"group-name": "er", "description": "Entity Relationship Scripts"}
            ],
        "command-sets": [
                {"pattern": "scripts/([a-z].*).pl", "group-name": "cs"},
                {"pattern": "scripts/(get_relationship.*).pl", "group-name": "er"},
                {"pattern": "scripts/(get_entity.*).pl", "group-name": "er"},
                {"pattern": "scripts/(all_entities.*).pl", "group-name": "er"},
                {"pattern": "scripts/(query_entity.*).pl", "group-name": "er"}
            ],
        "commands": [
		{"command-name": "er-get-entitiy-Tree", "group-name": "er"}
	    ]
	},

    "cli": {
        "commands": [
                {"file": "scripts-er/er-all-entities*.pl", "lang":"perl", "iris-group-name": "er"},
                {"file": "scripts-er/er-get-relationship*.pl", "lang":"perl", "iris-group-name": "er"},
                {"file": "scripts-er/er-get-entity*.pl", "lang":"perl", "iris-group-name": "er"},
                {"file": "scripts-er/er-query-entity*.pl", "lang":"perl", "iris-group-name": "er"}
            ],
        "deprecated-commands": [
                {
                    "deprecated-name": "all_entities_genome",
                    "file-name": "scripts-er/er-all-entities-Genome.pl",
                    "lang":"perl",
                    "deploy-to-iris":true,
                    "new-command-name": "er-all-entities-Genome",
                    "warning-mssg": "blah blah blah"
                }
            ]
    }
}